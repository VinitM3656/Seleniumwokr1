package May01;

public class Wrapper {
	public static void main(String [] args) {
		
		int a = 34;/// data type adn tych object type krych very simple
		Integer IntObj = new Integer(a);
		
		char c = 'd';
		Character Ch = new Character(c);
		 
		
		String str = " vinit";
		String str1 = new String (str);
		
		
		
		Double Doub =23.34;
		Double Doub1 = new Double(Doub);
		
		
		Float f = 223.3f;
		Float Fobj = new Float(f);
		
		
		
		System.out.println(" the wrapper object valus is "+ (a));
		
		System.out.println(" the wrapper value os char is "+(c));
		
		System.out.println(" the wrapper value is doub"+(Doub));
		
		
		
		
	
		
	}

}
