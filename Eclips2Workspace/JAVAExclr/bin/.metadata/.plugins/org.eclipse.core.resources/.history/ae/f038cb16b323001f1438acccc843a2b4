package CodeSikho;

class Rectangle{
	
	int L,b;
	
	void setData(int l ,int b) {
		this.L=l;
		this.b=b;
		
	}
	
	void display() {
		System.out.println(this.L);
		System.out.println(this.b);
		
		
	}
}






public class This {
public static void main(String [] args) {
	
	Rectangle r1 = new Rectangle();
	r1.setData(23, 230);
	
	r1.display();
	 
	
}
}
